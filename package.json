{
  "name": "gitty-gitty-git-er",
  "version": "1.0.0",
  "description": "A comprehensive GitHub bot for managing repositories, code, and more with standalone and PWA capabilities",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "chatbot": "cd chatbot && node chatbot.js",
    "build-exe": "cd chatbot && node build.js",
    "web:dev": "cd web && npm run dev",
    "web:build": "cd web && npm run build",
    "pwa:build": "cd web && npm run build && cp -r ../pwa/service-worker.js ../pwa/manifest.json web/dist/",
    "electron:start": "cd electron && npm start",
    "electron:build": "npm run web:build && cd electron && npm run build",
    "postinstall": "npm run install:all",
    "install:all": "npm run install:chatbot && npm run install:web && npm run install:electron",
    "install:chatbot": "cd chatbot && npm install",
    "install:web": "cd web && npm install",
    "install:electron": "cd electron && npm install"
  },
  "keywords": [
    "github",
    "bot",
    "octokit",
    "automation",
    "pwa",
    "standalone"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "cookie-parser": "^1.4.6",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "pkg": "^5.8.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/940smiley/gitty-gitty-git-er.git"
  },
  "bugs": {
    "url": "https://github.com/940smiley/gitty-gitty-git-er/issues"
  },
  "homepage": "https://github.com/940smiley/gitty-gitty-git-er#readme"
}